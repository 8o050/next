// import React from "react";
import './styles.css';
import {State} from "../index";
import Plot from 'react-plotly.js';

const s = props.state as State;

const graphDiv = document.getElementById('id_of_the_div')

const data = {[
        x: s.x,
        y: s.y,
        type: 'scatter',
        mode: 'lines+markers',
        marker: {color: 'red'}
        ]};

const layout = {

    width: 960,
    height: 600,
    title: 'Ploty Plot Plot'

};

return ( <div> Plotly.newPlot(graphDiv, data, layout); </div> )

export default plots;
