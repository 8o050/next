// We are in production mode
{
  production: false,

  // The name of the server
  serverName: 'Localhost',

  // HTTP
  port: 3333,
  tlsVersion: "TLSv1.2",

  // Folders
  root: ".",
  public: "dist/apps/homebase/assets",
  assets: "dist/apps/webapp/assets/",

  // Services
  redis: {
    host: "localhost",
  },

  // Active SAGE3 servers accessible at login page
  servers: [
    { name: "Localhost", url: "http://localhost:4200"},
    { name: "SAGE3 Main Server", url: "https://sage3.app"},
    { name: "MacMini - EVL", url: "https://mini.sage3.app"},
    { name: "Pele", url: "https://pele.sage3.app"},
  ],

  // ID Management
  keys: {
    strategies: ['local', 'guest', 'jwt'],
    google: {
      clientId: '',
      clientSecret: '',
    },
    jwt: {
      // found in the 'keys' folder
      publicKey: 'jwt_public.pem',
    }
    session: {
      cookieKey: 'cookie_key_setup',
    },
    encryption: {
      cryptr: 'unique_key_for_crypter',
    },
    ssl: {
      // found in the 'keys' folder
      certificateFile: "localhost-server.crt",
      certificateChainFile: "localhost-ca.crt",
      certificateKeyFile: "localhost-server.key"
    }
  },

}
